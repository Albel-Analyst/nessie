              <goal>integration-test</goal>
              <goal>verify</goal>
            </goals>
          </execution>
        </executions>
        <configuration>
          <systemPropertyVariables>
            <quarkus.http.test-port>${quarkus.http.test-port}</quarkus.http.test-port>
          </systemPropertyVariables>
        </configuration>
      </plugin>
    </plugins>
